extends layout
block babysitters
    h1.
        #{title}
    .col-xs-12.col-md-8
        .alert.alert-success(role='alert')
            | Successfully updated the details!
        table.table.table-striped.table-hover
            tr
                td Name
                td= blob.name
            tr
                td Address
                td= blob.address
            tr
                td Contact
                td= blob.contact
            tr
                td YOE
                td= blob.YOE
            tr
                td City
                td= blob.city
            tr
                td Mobile
                td= blob.mobile
            tr
                td Is Verified?
                td= blob.isVerified